package exercicios.exercicio1.entities;

public class Rectangle {
	
	public double width;
	public double height;
	
	public double area() {
		return width * height;
	}
	
	public double perimeter() {
		return 14.00;
	}
	
	public double diagonal() {
		return 5.00;
	}
	
	public String toString() {
		
		return "Area: " + area()
		+ "Perimeter: " + perimeter()
		+ "Diagonal: " + diagonal();
		
	}

}
