package exercicios.exercicioFix;

public class Account {
	
	private int number;
	private String name;
	private double amount;
	
	public Account(int number, String name, double amount) {
		this.number = number;
		this.name = name;
		this.amount = amount;
	}

	public Account(int number, String name) {
		this.number = number;
		this.name = name;
	}
	
	
	
	
	

}
